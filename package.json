{
  "name": "@emscripten-forge/untarjs",
  "version": "1.0.0",
  "description": "A simple project to unzip .zip, .tar.bz2 and .conda files in the front-end",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build:wasm": "node build_wasm.js",
    "clean": "rimraf dist",
    "build": "yarn run build:wasm && tsc",
    "build:prod": "tsc && yarn run clean && yarn run build:wasm"
  },
  "files": [
     "lib/index.js",
     "lib/helper.js",
     "lib/index.d.ts",
     "lib/helper.d.ts",
     "lib/unpack.js",
     "lib/unpack.wasm",
     "src/unpack.d.ts",
     "src/unpack.wasm.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "untarjs",
    "unpack",
    "jupyter",
    "jupyterlab"
  ],
  "author": "Anastasiia Sliusar",
  "license": "MIT",
  
  "dependencies": {
    "assert": "^2.1.0",
    "process": "^0.11.10",
    "typescript": "^5",
    "url": "^0.11.4",
    "rimraf": "^3.0.2"
  }
}
